   "source": [
    "# Interactive Capacity Planning with Monte Carlo Simulation\n",
    "\n",
    "This notebook demonstrates interactive Monte Carlo simulation using **ipywidgets**.\n",
    "\n",
    "Adjust parameters in real-time and see results update instantly!"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "from ipywidgets import interact, interactive, fixed, interact_manual\n",
    "import ipywidgets as widgets\n",
    "from IPython.display import display, clear_output\n",
    "\n",
    "%matplotlib inline"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 1. Interactive Simulation Parameters\n",
    "\n",
    "Use sliders to adjust simulation parameters:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Create interactive controls\n",
    "style = {'description_width': '150px'}\n",
    "layout = widgets.Layout(width='500px')\n",
    "\n",
    "time_horizon_slider = widgets.IntSlider(\n",
    "    value=30, min=7, max=90, step=7,\n",
    "    description='Days to Simulate:',\n",
    "    style=style, layout=layout\n",
    ")\n",
    "\n",
    "simulations_slider = widgets.IntSlider(\n",
    "    value=5000, min=1000, max=20000, step=1000,\n",
    "    description='N Simulations:',\n",
    "    style=style, layout=layout\n",
    ")\n",
    "\n",
    "base_throughput_slider = widgets.IntSlider(\n",
    "    value=100, min=50, max=200, step=10,\n",
    "    description='Base Throughput:',\n",
    "    style=style, layout=layout\n",
    ")\n",
    "\n",
    "efficiency_slider = widgets.FloatSlider(\n",
    "    value=0.90, min=0.70, max=0.99, step=0.01,\n",
    "    description='Mean Efficiency:',\n",
    "    style=style, layout=layout\n",
    ")\n",
    "\n",
    "downtime_slider = widgets.FloatSlider(\n",
    "    value=0.03, min=0.0, max=0.1, step=0.01,\n",
    "    description='Downtime Prob:',\n",
    "    style=style, layout=layout,\n",
    "    readout_format='.2%'\n",
    ")\n",
    "\n",
    "display(time_horizon_slider, simulations_slider, base_throughput_slider, efficiency_slider, downtime_slider)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 2. Run Interactive Simulation\n",
    "\n",
    "Click the **Run Simulation** button to execute with your parameters:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def run_monte_carlo(time_horizon_days, n_simulations, base_throughput, efficiency_mean, downtime_prob):\n",
    "    \"\"\"Run Monte Carlo simulation with given parameters\"\"\"\n",
    "    results = []\n",
    "    \n",
    "    for _ in range(n_simulations):\n",
    "        total_output = 0\n",
    "        \n",
    "        for day in range(time_horizon_days):\n",
    "            is_down = np.random.random() < downtime_prob\n",
    "            \n",
    "            if not is_down:\n",
    "                efficiency = np.random.normal(efficiency_mean, 0.03)\n",
    "                efficiency = np.clip(efficiency, 0.3, 1.0)\n",
    "                daily_output = base_throughput * efficiency * 24\n",
    "                daily_output *= np.random.normal(1.0, 0.02)\n",
    "                total_output += daily_output\n",
    "        \n",
    "        results.append(total_output)\n",
    "    \n",
    "    return np.array(results)\n",
    "\n",
    "def simulate_and_plot(time_horizon, n_simulations, base_throughput, efficiency, downtime):\n",
    "    \"\"\"Run simulation and display results\"\"\"\n",
    "    clear_output(wait=True)\n",
    "    \n",
    "    print(f\"Running {n_simulations:,} simulations for {time_horizon} days...\")\n",
    "    \n",
    "    results = run_monte_carlo(time_horizon, n_simulations, base_throughput, efficiency, downtime)\n",
    "    \n",
    "    # Statistics\n",
    "    mean_val = np.mean(results)\n",
    "    std_val = np.std(results)\n",
    "    p5 = np.percentile(results, 5)\n",
    "    p95 = np.percentile(results, 95)\n",
    "    p99 = np.percentile(results, 99)\n",
    "    \n",
    "    fig, axes = plt.subplots(1, 2, figsize=(14, 5))\n",
    "    \n",
    "    # Histogram\n",
    "    axes[0].hist(results, bins=50, edgecolor='black', alpha=0.7, color='steelblue')\n",
    "    axes[0].axvline(mean_val, color='red', linestyle='--', linewidth=2, label=f'Mean: {mean_val:.0f}')\n",
    "    axes[0].axvline(p5, color='orange', linestyle='--', linewidth=2, label=f'P5: {p5:.0f}')\n",
    "    axes[0].axvline(p95, color='orange', linestyle='--', linewidth=2, label=f'P95: {p95:.0f}')\n",
    "    axes[0].axvline(p99, color='green', linestyle='--', linewidth=2, label=f'P99: {p99:.0f}')\n",
    "    axes[0].set_xlabel('Total Throughput (wafers)')\n",
    "    axes[0].set_ylabel('Frequency')\n",
    "    axes[0].set_title(f'Throughput Distribution ({n_simulations:,} sims)')\n",
    "    axes[0].legend()\n",
    "    \n",
    "    # Box plot\n",
    "    axes[1].boxplot(results, vert=True)\n",
    "    axes[1].set_ylabel('Throughput (wafers)')\n",
    "    axes[1].set_title('Summary Statistics')\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "    \n",
    "    # Summary table\n",
    "    summary_df = pd.DataFrame({\n",
    "        'Metric': ['Mean', 'Std Dev', 'Min', 'P5', 'P50 (Median)', 'P95', 'P99', 'Max'],\n",
    "        'Value (wafers)': [\n",
    "            f\"{mean_val:.0f}\",\n",
    "            f\"{std_val:.0f}\",\n",
    "            f\"{np.min(results):.0f}\",\n",
    "            f\"{p5:.0f}\",\n",
    "            f\"{np.median(results):.0f}\",\n",
    "            f\"{p95:.0f}\",\n",
    "            f\"{p99:.0f}\",\n",
    "            f\"{np.max(results):.0f}\"\n",
    "        ]\n",
    "    })\n",
    "    display(summary_df)\n",
    "\n",
    "# Interactive button\n",
    "run_button = widgets.Button(\n",
    "    description='ðŸš€ Run Simulation',\n",
    "    button_style='success',\n",
    "    layout=widgets.Layout(width='200px', height='40px')\n",
    ")\n",
    "\n",
    "def on_run_clicked(b):\n",
    "    simulate_and_plot(\n",
    "        time_horizon_slider.value,\n",
    "        simulations_slider.value,\n",
    "        base_throughput_slider.value,\n",
    "        efficiency_slider.value,\n",
    "        downtime_slider.value\n",
    "    )\n",
    "\n",
    "run_button.on_click(on_run_clicked)\n",
    "display(run_button)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 3. Real-Time Sensitivity Analysis\n",
    "\n",
    "Explore how changing one parameter affects the distribution:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "@interact(\n",
    "    efficiency_range=(0.70, 0.99, 0.05),\n",
    "    time_horizon=fixed(30),\n",
    "    n_sims=fixed(2000)\n",
    ")\n",
    "def sensitivity_analysis(efficiency_range, time_horizon, n_sims):\n",
    "    \"\"\"Compare throughput at different efficiency levels\"\"\"\n",
    "    efficiencies = [efficiency_range - 0.1, efficiency_range, efficiency_range + 0.05]\n",
    "    efficiencies = [max(0.7, min(0.99, e)) for e in efficiencies]\n",
    "    colors = ['coral', 'steelblue', 'seagreen']\n",
    "    \n",
    "    plt.figure(figsize=(12, 6))\n",
    "    \n",
    "    for eff, color in zip(efficiencies, colors):\n",
    "        results = run_monte_carlo(time_horizon, n_sims, 100, eff, 0.03)\n",
    "        plt.hist(results, bins=40, alpha=0.5, color=color,\n",
    "                label=f'Efficiency: {eff:.0%} (Î¼={np.mean(results):.0f})')\n",
    "    \n",
    "    plt.xlabel('Total Throughput (wafers)')\n",
    "    plt.ylabel('Frequency')\n",
    "    plt.title(f'Sensitivity Analysis: Efficiency Impact ({time_horizon} days)')\n",
    "    plt.legend()\n",
    "    plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 4. Multi-Machine Configuration Dashboard\n",
    "\n",
    "Configure multiple machines with interactive widgets:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Machine configuration widgets\n",
    "machine_tabs = widgets.Tab()\n",
    "\n",
    "machine_configs = []\n",
    "children = []\n",
    "\n",
    "for i in range(3):\n",
    "    name = widgets.Text(value=f'Machine-{i+1}', description='Name:')\n",
    "    throughput = widgets.IntSlider(value=100, min=50, max=200, description='Throughput:')\n",
    "    efficiency = widgets.FloatSlider(value=0.90, min=0.7, max=0.99, step=0.01, description='Efficiency:')\n",
    "    downtime = widgets.FloatSlider(value=0.03, min=0.0, max=0.1, step=0.01, description='Downtime %:', readout_format='.1%')\n",
    "    \n",
    "    box = widgets.VBox([name, throughput, efficiency, downtime])\n",
    "    children.append(box)\n",
    "    machine_configs.append({'name': name, 'throughput': throughput, 'efficiency': efficiency, 'downtime': downtime})\n",
    "\n",
    "machine_tabs.children = children\n",
    "for i in range(3):\n",
    "    machine_tabs.set_title(i, f'Machine {i+1}')\n",
    "\n",
    "display(machine_tabs)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Run multi-machine simulation\n",
    "def run_multi_machine_simulation(days=30, n_sims=2000):\n",
    "    \"\"\"Run simulation with configured machines\"\"\"\n",
    "    machines = []\n",
    "    for config in machine_configs:\n",
    "        machines.append({\n",
    "            'name': config['name'].value,\n",
    "            'base_throughput': config['throughput'].value,\n",
    "            'efficiency_mean': config['efficiency'].value,\n",
    "            'downtime_prob': config['downtime'].value\n",
    "        })\n",
    "    \n",
    "    # Simulate each machine\n",
    "    results_by_machine = {}\n",
    "    for machine in machines:\n",
    "        results = run_monte_carlo(\n",
    "            days, n_sims,\n",
    "            machine['base_throughput'],\n",
    "            machine['efficiency_mean'],\n",
    "            machine['downtime_prob']\n",
    "        )\n",
    "        results_by_machine[machine['name']] = results\n",
    "    \n",
    "    # Plot comparison\n",
    "    fig, axes = plt.subplots(1, 2, figsize=(14, 5))\n",
    "    \n",
    "    # Individual distributions\n",
    "    for name, results in results_by_machine.items():\n",
    "        axes[0].hist(results, bins=30, alpha=0.5, label=f'{name} (Î¼={np.mean(results):.0f})')\n",
    "    axes[0].set_xlabel('Throughput (wafers)')\n",
    "    axes[0].set_ylabel('Frequency')\n",
    "    axes[0].set_title('Throughput by Machine')\n",
    "    axes[0].legend()\n",
    "    \n",
    "    # Total factory output\n",
    "    total_output = np.sum(list(results_by_machine.values()), axis=0)\n",
    "    axes[1].hist(total_output, bins=40, color='purple', alpha=0.7, edgecolor='black')\n",
    "    axes[1].axvline(np.mean(total_output), color='red', linestyle='--', linewidth=2,\n",
    "                    label=f'Mean: {np.mean(total_output):.0f}')\n",
    "    axes[1].set_xlabel('Total Factory Output (wafers)')\n",
    "    axes[1].set_ylabel('Frequency')\n",
    "    axes[1].set_title(f'Combined Factory Output ({days} days)')\n",
    "    axes[1].legend()\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "    \n",
    "    # Summary\n",
    "    print(\"\\n=== Machine Summary ===\")\n",
    "    for name, results in results_by_machine.items():\n",
    "        print(f\"{name}: Î¼={np.mean(results):.0f}, Ïƒ={np.std(results):.0f}, P95={np.percentile(results, 95):.0f}\")\n",
    "    print(f\"\\nTotal Factory: Î¼={np.mean(total_output):.0f}, Ïƒ={np.std(total_output):.0f}\")\n",
    "\n",
    "multi_run_btn = widgets.Button(\n",
    "    description='ðŸ­ Run Factory Simulation',\n",
    "    button_style='primary',\n",
    "    layout=widgets.Layout(width='220px', height='40px')\n",
    ")\n",
    "multi_run_btn.on_click(lambda b: run_multi_machine_simulation())\n",
    "display(multi_run_btn)"
   ]
  },
  {
